{
    "ver": "0.0.1",
    "authors": [
        "marcin"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "kbaseReportView"
    },
    "parameters": [ 
        {
            "id": "group-name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "string"
            }
        },
        {
            "id": "description",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 0
            }
        },
	{

            "id": "reads-objects-single",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseAssembly.SingleEndLibrary" ],
                "min_integer" : 0
            }
        },
	{

            "id": "reads-objects-paired",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseAssembly.PairedEndLibrary" ],
                "min_integer" : 0
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "ReadGroupEditor",
            "method": "filter_contigs",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace"
                },
                {
                    "input_parameter": "group-name",
                    "target_property": "group-name"
                },
                {
                    "input_parameter": "description",
                    "target_property": "description"
                },
                {
                    "input_parameter": "reads-objects-single",
                    "target_property": "reads-objects-single"
                },
                {
                    "input_parameter": "reads-objects-paired",
                    "target_property": "reads-objects-paired"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "constant_value": "5",
                    "target_property": "report_window_line_height"
                },
                {
                    "service_method_output_path": [0],
                    "target_property": "group_editor_result"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
